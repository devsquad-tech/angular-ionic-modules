{
  "name": "@devsquad/angular-ionic-modules",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "prebuild": "npm run clean:dist",
    "build": "node ./scripts/run-scope build",
    "precanary": "npm run build",
    "canary": "node ./scripts/run-scope canary",
    "clean:dist": "lerna exec -- rimraf ./dist",
    "clean:modules": "lerna clean",
    "clean": "npm run clean:modules && npm run clean:dist",
    "commit": "git-cz",
    "commitlint": "commitlint",
    "commitmsg": "commitlint -e",
    "precoverage": "npm run clean:dist",
    "coverage": "node ./scripts/run-scope test --coverage",
    "link": "lerna link",
    "lint": "lerna exec -- tslint --config ../../tslint.json --project ../../tsconfig.json --type-check",
    "precommit": "npm run lint",
    "prepublish": "npm run build",
    "publish": "node ./scripts/run-scope publish",
    "prepush": "npm run test:ci",
    "pretest": "npm run clean:dist",
    "test": "node ./scripts/run-scope test",
    "pretest:ci": "npm run clean:dist",
    "test:ci": "node ./scripts/run-scope test --ci"
  },
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@commitlint/cli": "^3.2.0",
    "@commitlint/config-angular": "^3.1.1",
    "@types/jasmine": "2.2.33",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@devsquad/tslint-rules": "0.0.3",
    "browserify": "^14.4.0",
    "commitizen": "^2.9.6",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-preset-angular": "^4.0.0",
    "lerna": "^2.0.0",
    "rimraf": "^2.5.4",
    "rxjs": "^5.4.1",
    "typescript": "^2.4.2",
    "watchify": "^3.9.0"
  }
}
